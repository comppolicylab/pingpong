        <mxCell id="-qnXSBUMEC2PwjZ8c9BN-53" value="&lt;h1 style=&quot;margin-top: 0px;&quot;&gt;ELB&lt;/h1&gt;&lt;p&gt;&amp;nbsp;- Terminates HTTPS&lt;/p&gt;&lt;p&gt;&amp;nbsp;- Directs traffic to upstream targets in Fargate, either API or UI servers, depending on route.&lt;br&gt;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="139" y="1221" width="311" height="251" as="geometry" />
        </mxCell>
        <mxCell id="-qnXSBUMEC2PwjZ8c9BN-54" value="&lt;h1 style=&quot;margin-top: 0px;&quot;&gt;API server&lt;br&gt;&lt;/h1&gt;&lt;p&gt;&amp;nbsp;- Python server&lt;/p&gt;&lt;p&gt;&amp;nbsp;- Scaled to N replicas based on load (min=2)&lt;br&gt;&lt;br&gt;&amp;nbsp;- Issues and validates signed JWTs for authentication&lt;br&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp;- Defers to Authz server for permissions&lt;/p&gt;&lt;p&gt;&amp;nbsp;- Coordinates data between OpenAI, Postgres, etc&lt;br&gt;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="139" y="992" width="311" height="251" as="geometry" />
        </mxCell>
        <mxCell id="-qnXSBUMEC2PwjZ8c9BN-55" value="&lt;h1 style=&quot;margin-top: 0px;&quot;&gt;UI server&lt;br&gt;&lt;/h1&gt;&lt;p&gt;&amp;nbsp;- NodeJS server&lt;br&gt;&lt;br&gt;&amp;nbsp;- Scaled to N replicas based on load (min=2)&lt;br&gt;&lt;/p&gt;&lt;p&gt;&amp;nbsp;- Prerenders UI for improved experience&lt;/p&gt;&lt;p&gt;&amp;nbsp;- Serves static assets&lt;/p&gt;&lt;p&gt;&amp;nbsp;- Fetches data from API&lt;br&gt;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="456" y="992" width="311" height="251" as="geometry" />
        </mxCell>
        <mxCell id="-qnXSBUMEC2PwjZ8c9BN-56" value="&lt;h1 style=&quot;margin-top: 0px;&quot;&gt;Authz server&lt;br&gt;&lt;/h1&gt;&lt;p&gt;&amp;nbsp;- OpenFGA server&lt;/p&gt;&lt;p&gt;&amp;nbsp;- Scaled to N replicas based on load (min=2)&lt;/p&gt;&lt;p&gt;&amp;nbsp;- Judges permission based on ReBAC model&lt;/p&gt;&lt;p&gt;&amp;nbsp;- Lists visible objects based on ReBAC model&lt;br&gt;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="764" y="992" width="311" height="251" as="geometry" />
        </mxCell>
        <mxCell id="-qnXSBUMEC2PwjZ8c9BN-57" value="&lt;h1 style=&quot;margin-top: 0px;&quot;&gt;RDS&lt;br&gt;&lt;/h1&gt;&lt;p&gt;&amp;nbsp;- Postgres DB&lt;/p&gt;&lt;p&gt;&amp;nbsp;- With standby for backup&lt;/p&gt;&lt;p&gt;&amp;nbsp;- Two schemas, one for app data one for authz tuples&lt;/p&gt;&lt;p&gt;&amp;nbsp;- Stores email addresses, but no other identifiable user data&lt;br&gt;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="456" y="1221" width="311" height="251" as="geometry" />
        </mxCell>
        <mxCell id="-qnXSBUMEC2PwjZ8c9BN-58" value="&lt;h1 style=&quot;margin-top: 0px;&quot;&gt;Mailhub&lt;br&gt;&lt;/h1&gt;&lt;p&gt;&amp;nbsp;- Sends authentication tokens to user email for login (magic link)&lt;/p&gt;&lt;p&gt;&amp;nbsp;- Sends invitation emails when users added to class&lt;br&gt;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
          <mxGeometry x="1072" y="1221" width="311" height="251" as="geometry" />
        </mxCell>
        <mxCell id="-qnXSBUMEC2PwjZ8c9BN-59" value="&lt;h1 style=&quot;margin-top: 0px;&quot;&gt;OpenAI&lt;br&gt;&lt;/h1&gt;&lt;p&gt;&amp;nbsp;- Stores conversation thread content via Assistants API&lt;/p&gt;&lt;p&gt;&amp;nbsp;- Generates responses to user input&lt;br&gt;&lt;/p&gt;" style="text;html=1;whiteSpace=wrap;overflow=hidden;rounded=0;" vertex="1" parent="1">
